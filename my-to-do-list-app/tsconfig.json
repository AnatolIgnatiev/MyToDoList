{
    "compilerOptions": {
        "target": "ES2015",
        "lib": [
            "dom",
            "dom.iterable",
            "ES2015"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "esnext",
        "moduleResolution": "node",
        "isolatedModules": true,
        "noEmit": true,
        "jsx": "react-jsx",
        "resolveJsonModule": true
    },
    "exclude": [
        "wwwroot/lib"
    ],
  "include": [
    "src",
    "node_modules"
  ]
}
